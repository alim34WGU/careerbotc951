	<!-- Generates the Career Option(s) of the Student -->
	<category>
	    <pattern>ZMAXREPORT <set>number</set></pattern>
	    
	    <template>
	        <think><set var = "maxvalue"> <star/> </set></think>
	        <think><set var = "commavalue"> 0 </set></think>
	        <think><set var = "placeholder"> 0 </set></think>
	        
	        <condition var = "placeholder">
	            <li>you should consider the following career option(s): </li>
	        </condition>
	         
	        <condition var = "maxvalue">
	            <li><value><get name = "management"/></value>
	                <think><set var = "commavalue"> 1 </set></think>
	                 IT management
	            </li>
	        </condition>
	        
	        <condition var = "maxvalue">
	            <li><value><get name = "consulting"/></value>
	                <condition var = "commavalue">
	                    <li value = "1">, </li>
	                    <li></li>
	                </condition>
	                
	                <think><set var = "commavalue"> 1 </set></think>
	                software consulting
	            </li>
	       </condition>
	       
	       <condition var = "maxvalue">
	            <li><value><get name = "development"/></value>
	                <condition var = "commavalue">
	                    <li value = "1">, </li>
	                    <li></li>
	                </condition>
	                
	                <think><set var = "commavalue"> 1 </set></think>
	                software development
	            </li>
	       </condition>
	       
	       <condition var = "maxvalue">
	            <li><value><get name = "security"/></value>
	                <condition var = "commavalue">
	                    <li value = "1">, </li>
	                    <li></li>
	                </condition>
	                
	                <think><set var = "commavalue"> 1 </set></think>
	                cybersecurity
	            </li>
	       </condition>
	       
	       <condition var = "maxvalue">     
	            <li><value><get name = "analysis"/></value>
	                <condition var = "commavalue">
	                    <li value = "1">, </li>
	                    <li></li>
	                </condition>
	                
	                <think><set var = "commavalue"> 1 </set></think>
	                data analysis
	            </li>
	       </condition>
	        
	        .
	        
	    </template>
	</category>
</aiml>