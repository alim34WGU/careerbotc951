<?xml version="1.0" encoding="UTF-8"?>
<aiml version="2.0">
	<!-- insert your AIML categories here -->
	<category>
	    <pattern>
	        DO YOUR THING
	    </pattern>
	    <template>
	        <think><set var="num">17</set></think>
	        <srai> XADD <get var="num" /> XS 34 </srai>
	    </template>
	</category>

    <category>
	    <pattern>
	        ZMAX 0 ZS <set>number</set>
	    </pattern>
	    <template>
	        <think> <set var = "score"> <star /> </set> </think>
	        <condition var="score">
	            <li value = "0"> BOTH </li>
	            <li> RIGHT </li>
	        </condition>
	    </template>
	</category>
	
	<category>
	    <pattern>
	        ZMAX <set>number</set> ZS 0
	    </pattern>
	    <template>
	        <think> <set var = "score"> <star /> </set> </think>
	        <condition var="score">
	            <li value = "0"> BOTH </li>
	            <li> LEFT </li>
	        </condition>
	    </template>
	</category>
	
    <category>
	    <pattern>
	        ZMAX <set>number</set> ZS <set>number</set>
	    </pattern>
	    <template>
	        <srai> ZMAX  <map name="predecessor"><star index="1" /></map> ZS <map name="predecessor"><star index="2" /></map> </srai>
	    </template>
	</category>
	
	<category>
	    <pattern>
	        FMAX <set>number</set> FS <set>number</set>
	    </pattern>
	    <template>
	        <think><set name = "maxvalue"><srai> ZMAX  <star index="1" /> ZS <star index="2" /> </srai></set></think>
	         <condition name="maxvalue">
	            <li value = "LEFT"> <star index="1" /> </li>
	            <li> <star index="2" /> </li>
	        </condition>
	    </template>
	</category>
	
</aiml>