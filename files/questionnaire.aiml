<?xml version="1.0" encoding="UTF-8"?>
<aiml version="2.0">
	<!-- insert your AIML categories here -->
	<category>
	    <pattern>
	        DO YOUR THING
	    </pattern>
	    <template>
	        <think><set var="num">17</set></think>
	        <srai> XADD <get var="num" /> XS 34 </srai>
	    </template>
	</category>

    <category>
	    <pattern>
	        ZMAX 0 ZS <set>number</set>
	    </pattern>
	    <template>
	        <think> <set var = "score"> <star /> </set> </think>
	        <condition var="score">
	            <li value = "0"> BOTH </li>
	            <li> RIGHT </li>
	        </condition>
	    </template>
	</category>
	
	<category>
	    <pattern>
	        ZMAX <set>number</set> ZS 0
	    </pattern>
	    <template>
	        <think> <set var = "score"> <star /> </set> </think>
	        <condition var="score">
	            <li value = "0"> BOTH </li>
	            <li> LEFT </li>
	        </condition>
	    </template>
	</category>
	
    <category>
	    <pattern>
	        ZMAX <set>number</set> ZS <set>number</set>
	    </pattern>
	    <template>
	        <srai> ZMAX  <map name="predecessor"><star index="1" /></map> ZS <map name="predecessor"><star index="2" /></map> </srai>
	    </template>
	</category>
	
	<category>
	    <pattern>
	        FMAX <set>number</set> FS <set>number</set>
	    </pattern>
	    <template>
	        <think><set name = "maxvalue"><srai> ZMAX  <star index="1" /> ZS <star index="2" /> </srai></set></think>
	         <condition name="maxvalue">
	            <li value = "LEFT"> <star index="1" /> </li>
	            <li> <star index="2" /> </li>
	        </condition>
	    </template>
	</category>
	
	<category>
	    <pattern>
	        LMAX <set>number</set> LS <set>number</set> <set>number</set> <set>number</set> <set>number</set>
	    </pattern>
	    <template>
	        <think>
	            <set var = "maxnum"><srai> FMAX <star index="1" /> FS <star index="2" /> </srai></set>
	            <set var = "maxnum"><srai> FMAX <get var = "maxnum"/> FS <star index="3" /> </srai></set>
	            <set var = "maxnum"><srai> FMAX <get var = "maxnum"/> FS <star index="4" /> </srai></set>
	            <set var = "maxnum"><srai> FMAX <get var = "maxnum"/> FS <star index="5" /> </srai></set>
	        </think>
	        <get var = "maxnum"/>
	    </template>
	</category>
	
	<category>
	    <pattern>
	        ^ CAREER ^
	    </pattern>
	    <template>
	        <think>
	            <set name = "questionnum"> 1 </set>
	            <set name = "topic"> data1 </set>
	        </think>
	        Great! I will ask you a series of questions to find your strengths, preferences, and the best career option. <br/>
	        <srai> ZDATA1 </srai>
	    </template>
	</category>
	
	<topic name = "data1">
    <category>
	    <pattern>
	        ZDATA1
	    </pattern>
	    <template>
	        On a scale of 1 to 5, with 1 representing 'strongly disagree', 3 representing 'neither agree or disagree', and 5 representing 'strong agree', 
	        please indicate how much you agree or disagree with the following statement:<br/>
	        I love to apply numbers, metrics, mathematical or statistical concepts to my current tasks.
	    </template>
	</category>
	
	<category>
	    <pattern>
	        <set>number</set>
	    </pattern>
	    <template>
	        <think>
	            <set var = "rating"><star index = "1" /></set>
	        </think>
	        <condition var = "rating">
	            <li value = "1"> </li>
	            <li value = "2"> </li>
	            <li value = "3"> </li>
	            <li value = "4"> </li>
	            <li value = "5"> </li>
	            <li> Please enter a number between 1`and 5 inclusive. <srai>ZDATA1</srai></li>
	        </condition>
	    </template>
	</category>
	
	</topic>
	
</aiml>