	<!-- Creates a Report for the Student of His/Her Preferences and Strengths and Career Options 
	     If the student does not answer positively with most of the questions, then it is likely that
	     he or she may not have any preference, strength, or career option as determined by the questionnaire.
	     Note that the local variable 'placeholder' is needed so that its associated plain text can be printed
	     despite its value not being used.
	-->
	<topic name = "createreport">
	<category>
	    <pattern>
	        ZCREATEREPORT
	    </pattern>
	    <template>
	       <think><set var = "commavar"> 0 </set></think>
	       <think><set var = "placeholder"> 0 </set></think>
	       
	       <condition var = "placeholder">
	            <li>You are strong in the following area(s): </li>
	        </condition>
	       
	       <condition name = "numericskill">
	           <li value = "1"> 
	                <think><set var = "commavar"> 1 </set></think>
	                math, statistics, and logical reasoning
	           </li>
	           <li></li>
	       </condition>
	       
	       <condition name = "leadershipskill">
	           <li value = "1">
	                <condition var = "commavar">
	                    <li value = "1">, </li>
	                    <li></li>
	                </condition>
	                <think><set var = "commavar"> 1 </set></think>
	                leadership
	           </li>
	           <li></li>
	       </condition>
	       
	       <condition name = "problemsolveskill">
	           <li value = "1"> 
	                <condition var = "commavar">
	                    <li value = "1">, </li>
	                    <li></li>
	                </condition>
	                <think><set var = "commavar"> 1 </set></think>
	                solving people's problems
	           </li>
	           <li></li>
	       </condition>
	       
	       <condition name = "buildskill">
	           <li value = "1">
	                <condition var = "commavar">
	                    <li value = "1">, </li>
	                    <li></li>
	                </condition>
	                <think><set var = "commavar"> 1 </set></think>
	                creating objects
	           </li>
	           <li></li>
	       </condition>
	       
	       <condition name = "errordetectskill">
	           <li value = "1">
	               <condition var = "commavar">
	                    <li value = "1">, </li>
	                    <li></li>
	               </condition>
	               <think><set var = "commavar"> 1 </set></think>
	               eliminating errors
	           </li>
	           <li></li>
	       </condition>
	       
	       <condition var = "commavar">
	           <li value = "0"> none </li>
	           <li></li>
	       </condition>
	       .
	       
	       <think><set var = "commavar"> 0 </set></think>
	       
	       <condition var = "placeholder">
	            <li> Your preference(s) are as follows: </li>
	        </condition>
	        
	       <condition name = "numericpref">
	           <li value = "1"> 
	                <think><set var = "commavar"> 1 </set></think>
	                working with numbers
	           </li>
	           <li></li>
	       </condition>
	       
	       <condition name = "communicationpref">
