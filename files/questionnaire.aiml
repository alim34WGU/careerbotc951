<?xml version="1.0" encoding="UTF-8"?>
<aiml version="2.0">
	
	<!-- Helper Function to Determine Which of the Two Nonnegative Integers is Greater-->
    <category>
	    <pattern>
	        ZMAX 0 ZS <set>number</set>
	    </pattern>
	    <template>
	        <think> <set var = "score"> <star /> </set> </think>
	        <condition var="score">
	            <li value = "0"> BOTH </li>
	            <li> RIGHT </li>
	        </condition>
	    </template>
	</category>
	
	<!-- Helper Function to Determine Which of the Two Nonnegative Integers is Greater-->
	<category>
	    <pattern>
	        ZMAX <set>number</set> ZS 0
	    </pattern>
	    <template>
	        <think> <set var = "score"> <star /> </set> </think>
	        <condition var="score">
	            <li value = "0"> BOTH </li>
	            <li> LEFT </li>
	        </condition>
	    </template>
	</category>
	
	<!-- Determines the Greater of Two Nonnegative Integers -->
    <category>
	    <pattern>
	        ZMAX <set>number</set> ZS <set>number</set>
	    </pattern>
	    <template>
	        <srai> ZMAX  <map name="predecessor"><star index="1" /></map> ZS <map name="predecessor"><star index="2" /></map> </srai>
	    </template>
	</category>
	
	<!-- Determines the Max of Two Nonnegative Integers -->
	<category>
	    <pattern>
	        FMAX <set>number</set> FS <set>number</set>
	    </pattern>
	    <template>
	        <think><set name = "maxvalue"><srai> ZMAX  <star index="1" /> ZS <star index="2" /> </srai></set></think>
	         <condition name="maxvalue">
	            <li value = "LEFT"> <star index="1" /> </li>
	            <li> <star index="2" /> </li>
	        </condition>
	    </template>
	</category>
	
	<!-- Determines the Max of Five Nonnegative Integers -->
	<category>
	    <pattern>
	        LMAX <set>number</set> LS <set>number</set> <set>number</set> <set>number</set> <set>number</set>
	    </pattern>
	    <template>
	        <think>
	            <set var = "maxnum"><srai> FMAX <star index="1" /> FS <star index="2" /> </srai></set>
	            <set var = "maxnum"><srai> FMAX <get var = "maxnum"/> FS <star index="3" /> </srai></set>
	            <set var = "maxnum"><srai> FMAX <get var = "maxnum"/> FS <star index="4" /> </srai></set>
	            <set var = "maxnum"><srai> FMAX <get var = "maxnum"/> FS <star index="5" /> </srai></set>
	        </think>
	        <get var = "maxnum"/>
	    </template>
	</category>
	
	<!-- Start of the Career Questionnaire
	     Creating Predicates to Store Skills and Preferences and to Determine the Best Career Option(s)
	     Three Questions for Five Career Areas: Software Development, Management, Consulting, Cybersecurity, 
	     Data Analysis
	     One of the Three Question in Each Career Area has an Associated Preference
	     One of the Three Question in Each Career Area has an Associated Strength
	-->
	<category>
	    <pattern>
	        ^ CAREER ^
	    </pattern>
	    <template>
	        <think>
	            <set name = "topic"> data1 </set>
	            
	            <set name = "numericskill"> 0 </set>
	            <set name = "leadershipskill"> 0 </set>
	            <set name = "problemsolveskill"> 0 </set>
	            <set name = "buildskill"> 0 </set>
	            <set name = "errordetectskill"> 0 </set>
	            
	            <set name = "numericpref"> 0 </set>
	            <set name = "communicationpref"> 0 </set>
	            <set name = "workplacepref"> 0 </set>
	            <set name = "codingpref"> 0 </set>
	            <set name = "securitypref"> 0 </set>
	            
	            <set name = "management"> 0 </set>
	            <set name = "consulting"> 0 </set>
	            <set name = "development"> 0 </set>
	            <set name = "security"> 0 </set>
	            <set name = "analysis"> 0 </set>
	        </think>
	        Great! I will ask you a series of questions to find your strengths, preferences, and the best career option(s). <br/>
	        <srai> ZDATA1 </srai>
	    </template>
	</category>
	
	<!-- Question 1 for Data Analysis -->
	<topic name = "data1">
    <category>
	    <pattern>
	        ZDATA1
	    </pattern>
	    <template>
	        On a scale of 1 to 5, with 1 representing 'strongly disagree', 3 representing 'neither agree nor disagree', and 5 representing 'strongly agree', 
	        please indicate how much you agree or disagree with the following statement:<br/>
	        I love to apply numbers, metrics, mathematical or statistical concepts to my current tasks.
	    </template>
	</category>
	
	<!-- Stores Specific Skill/Preference and Answer for Question -->
	<category>
	    <pattern>
	        *
	    </pattern>
	    <template>
	        <think>
	            <set var = "rating"><star index = "1" /></set>
	        </think>
	        <condition var = "rating">
	            <li value = "1">
	                <think>
	                    <set name = "analysis"><srai> XADD <get name = "analysis" /> XS <get var = "rating" /> </srai></set>
	                    <set name = "topic"> data2 </set>
	                </think>
	                <srai>ZDATA2</srai>
	            </li>
	            <li value = "2"> 
	                <think>
	                    <set name = "analysis"><srai> XADD <get name = "analysis" /> XS <get var = "rating" /> </srai></set>
	                    <set name = "topic"> data2 </set>
	                </think>
	                <srai>ZDATA2</srai>
	            </li>
	            <li value = "3">
	                <think>
	                    <set name = "analysis"><srai> XADD <get name = "analysis" /> XS <get var = "rating" /> </srai></set>
	                    <set name = "topic"> data2 </set>
	                </think>
	                <srai>ZDATA2</srai>
	            </li>
	            <li value = "4">
	                <think>
	                    <set name = "numericpref"> 1 </set>
    	                <set name = "analysis"><srai> XADD <get name = "analysis" /> XS <get var = "rating" /> </srai></set>
    	                <set name = "topic"> data2 </set>
	                </think>
	                <srai>ZDATA2</srai>
	            </li>
	            <li value = "5"> 
	                <think>
	                    <set name = "numericpref"> 1 </set>
    	                <set name = "analysis"><srai> XADD <get name = "analysis" /> XS <get var = "rating" /> </srai></set>
    	                <set name = "topic"> data2 </set>
